{
  "name": "zod-firebase-admin",
  "description": "zod firebase-admin",
  "author": {
    "name": "Valian",
    "url": "https://valian.ca",
    "organization": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valian-ca/zod-firebase-admin.git"
  },
  "keywords": [
    "zod",
    "firebase",
    "firebase-admin"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    }
  },
  "type": "module",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "version": "0.0.14",
  "scripts": {
    "prettier": "prettier --check .",
    "eslint": "eslint --ext .ts,.tsx,.cjs .",
    "type-check": "tsc",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "prepare": "husky install",
    "test": "jest --coverage",
    "serve": "firebase emulators:start --only firestore --project demo-tests",
    "test:integration": "npm exec jest -- --config=jest.integration.config.cjs --coverage",
    "test:integration:ci": "firebase emulators:exec --only firestore --project demo-tests 'npm exec jest -- --config=jest.integration.config.cjs --coverage'",
    "compile": "rm -rf ./lib && rollup -c",
    "prepack": "npm run compile"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.2",
    "@commitlint/config-conventional": "18.4.2",
    "@rollup/plugin-typescript": "11.1.5",
    "@tsconfig/node18": "18.2.2",
    "@types/jest": "29.5.8",
    "@valian/eslint-config": "0.0.20",
    "eslint": "8.54.0",
    "firebase-admin": "11.11.0",
    "firebase-functions-test": "3.1.0",
    "firebase-tools": "12.9.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "markdownlint-cli2": "0.10.0",
    "prettier": "3.1.0",
    "rollup": "4.5.0",
    "rollup-plugin-dts": "6.1.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "peerDependencies": {
    "firebase-admin": ">=11",
    "typescript": ">=5"
  },
  "dependencies": {
    "zod": "^3.21.4"
  },
  "engines": {
    "node": ">= 18.0.0"
  }
}
